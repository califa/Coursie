  	     padding:0 10px 10px 10px;
  	     float:left;
  	     nav {
  	        padding: 20px 0 0 12px;
  	        li {
  	            padding-bottom: 6px;
  	            a {
  	                font-style: italic;
  	                color: #5265a5;
  	            }

  	        }
  	     }
  	    }

        .todo_list {
            height:160px;
  		    width:460px;
  		    float:left;
  		}
	}
}


.adminarea {

    .spacer { height:20px }
    ul.stripelist {
        text-align:center;
    }



    a {
        color: #073c95;
        font-style: italic;
        &:hover {
        color: $blue;
        }
    }
    div.content {
        width:600px;
        margin: 0 auto;
    }

    nav { width: 600px;
        li { margin-right: 0x; }
        .logo { width: 250px; }
        a { color: #fff;
        &:hover { color: #fff; }
        }
    }

    section {
        width:100%;
    }
    div.section_container {
        width:100%;
    }
    h1 { text-align: center; display:block; width: 100%;}
    .section_adminmenu {
            //width: 400px;
        div.section_container {
        text-align:center;
        a { color: $brown; font-weight:bold}
        }
    }



    .actions {
      text-align: right;
      a {
        @include border-radius(5px,5px,5px,5px);
        background: $red;
        padding: 4px;
        color: $list1;
        margin: 0 2px;
        &:hover {
          background: $brown;
        }
      }
      &.testbar a {
        background: #073c95;
      }
    }
    .num {
      text-align: center;
    }
    .lister {
      text-align: left;
      padding-left: 10px;
    }

    table {
        width: 100%;
        @include border-radius(5px,5px,5px,5px);
        tr {
		// Checkers Coloring
        @include border-radius();
            td {
                 padding: 10px;
                 background: $list1;
                 &.testbar {
                    background:#dadce8;
                 }
            }

            &:nth-child(even) { td {background:$list2;
                    &.testbar {
                    background:#e9eaf5;
                 }

            } }

            .actions a:hover { background: $brown; }

            // Rounded Corners
            &:first-child {
                    td {
                        &:first-child {
                            @include border-radius(5px,0,0,0);
                        }
                        &:last-child {
                            @include border-radius(0,5px,0,0);
                        }
                    }
                }
            &:last-child {
                    @include border-radius();
                    td {
                        &:first-child {
                            @include border-radius(0,0,0,5px);
                        }
                        &:last-child {
                            @include border-radius(0,0,5px,0)
                        }
                    }
                }


             td {
                margin: 0;
                padding: 0 10px;
                height: 40px;
                text-align:left;
             }



        }

        th {
                height:20px;
                font: 8pt Arvo, Rockwell;
                text-transform: uppercase;
                text-align: center;
                background: #fff;
            }
	}

    .field_with_errors {
        color: $red;
    }

    table.toplist {
         tr:nth-child(2) {
           td:first-child { @include border-radius(5px,0,0,0); }
           td:last-child {@include border-radius(0,5px,0,0);}
         }
         tr:nth-child(2):only-child {
           td:first-child { @include border-radius(5px,0,0,5px); }
           td:last-child {@include border-radius(0,5px,5px,0);}
           td:first-child:last-child {@include border-radius(5px,5px,5px,5px);}
         }
    }
    table.sidelist {
        tr {
            &:first-child {
              td:first-child {
                @include border-radius(5px,0,0,0)
              }
            }
            &:last-child {
              td:first-child {
                @include border-radius(0,0,0,5px)
              }
            }
        }

    }
    li.reference-name {
        text-weight: bold;
    }
    div.actions { margin-right: -10px;}


    .errorExplanation {

        li {
            text-align: center;
            @include border-radius(5px, 5px, 5px, 5px);
            background: $red;
            color: #fff;
            margin-bottom: 10px;
            padding: 5px;
        }

    }
}


//Tool tip

         .tooltip {
         	display:none;
         	background:$list1;
         	@include border-radius(10px, 0, 10px, 10px);
            border: 4px solid #c6c5b6;
         	padding:10px;
         	width:200px;
         	font-size:11px;
            z-index:1000;
         	color:#fff;
            .tipdate {
                font: 12pt Arvo, Rockwell;
                color: $brown;
                display: block;
                margin-bottom:10px;
            }
            li {
                background: $red;
                @include border-radius(5px, 5px, 5px, 5px);
                padding: 5px;
                margin-bottom:10px;
                &:last-child {
                    margin:0;
                }
                &.testtip {
                    background: $blue;
                }
                em {
                    display: block;
                    text-decoration: underline;
                    margin-bottom:5px;
                }
            }
         }
