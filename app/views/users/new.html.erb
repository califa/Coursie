<% @page_title = 'Create User' %>

<%= link_to("<< Back to List", users_path, :class => 'back-link') %>

<div class="user new">
  <h2>Create User</h2>

  <%= form_for(@user) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f} ) %>
  
    <div class="form-buttons">
      <%= submit_tag('Create User') %>
    </div>
  
  <% end %>
</div>
